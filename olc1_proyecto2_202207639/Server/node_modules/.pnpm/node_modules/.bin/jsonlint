#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ponche/Documentos/COMPILADORES1/OLC1_Proyecto2_202207639/olc1_proyecto2_202207639/Server/node_modules/.pnpm/jsonlint@1.6.0/node_modules/jsonlint/lib/node_modules:/home/ponche/Documentos/COMPILADORES1/OLC1_Proyecto2_202207639/olc1_proyecto2_202207639/Server/node_modules/.pnpm/jsonlint@1.6.0/node_modules/jsonlint/node_modules:/home/ponche/Documentos/COMPILADORES1/OLC1_Proyecto2_202207639/olc1_proyecto2_202207639/Server/node_modules/.pnpm/jsonlint@1.6.0/node_modules:/home/ponche/Documentos/COMPILADORES1/OLC1_Proyecto2_202207639/olc1_proyecto2_202207639/Server/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/ponche/Documentos/COMPILADORES1/OLC1_Proyecto2_202207639/olc1_proyecto2_202207639/Server/node_modules/.pnpm/jsonlint@1.6.0/node_modules/jsonlint/lib/node_modules:/home/ponche/Documentos/COMPILADORES1/OLC1_Proyecto2_202207639/olc1_proyecto2_202207639/Server/node_modules/.pnpm/jsonlint@1.6.0/node_modules/jsonlint/node_modules:/home/ponche/Documentos/COMPILADORES1/OLC1_Proyecto2_202207639/olc1_proyecto2_202207639/Server/node_modules/.pnpm/jsonlint@1.6.0/node_modules:/home/ponche/Documentos/COMPILADORES1/OLC1_Proyecto2_202207639/olc1_proyecto2_202207639/Server/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../jsonlint/lib/cli.js" "$@"
else
  exec node  "$basedir/../jsonlint/lib/cli.js" "$@"
fi
